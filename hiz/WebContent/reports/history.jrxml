	<queryString>
		<![CDATA[SELECT a.id,
doc,
code_sap,
doc_type,
firstnames,
surnames,
born_date,
CASE gender
 WHEN 'W' THEN 'Femenino'
 WHEN 'M' THEN 'Masculino'
END AS gender,
id_occupation,
address,
zip_code,
neighborhood,
phone_number,
cell_number,
email,
CASE type_housing
 WHEN 'U' THEN 'Urbana'
 WHEN 'R' THEN 'Rural'
END AS type_housing,
IFNULL(guardian,'SIN INFORMACIÓN') guardian,
guardian_address,
guardian_relationship,
IFNULL(guardian_telephone,'SIN INFORMACIÓN') guardian_telephone,
obs,
cycle,
send_phone,
send_email,
send_postal,
send_sms,
sales_org,
id_user_create,
date_create,
id_user_modified,
date_modified,
id_profile,
a.id_country,
b.name country_name,
a.id_region,
c.name region_name,
a.id_city,
d.name city_name,
id_marital_state,
IFNULL(e.item_value,'SIN INFORMACIÓN') marital_name,
id_membership,
IFNULL(f.item_value,'SIN INFORMACIÓN') membership_name,
id_eps,
IFNULL(g.name,'SIN INFORMACIÓN') eps_name,
IFNULL(h.name,'SIN INFORMACIÓN') occupation_name,
IFNULL(YEAR(CURDATE())-YEAR(a.born_date) + IF(DATE_FORMAT(CURDATE(),'%m-%d') > DATE_FORMAT(a.born_date,'%m-%d'), 0, -1), 0) AS age
FROM crm_db.crm_patient a JOIN crm_db.crm_country b ON a.id_country = b.id
JOIN crm_db.crm_region c ON a.id_region = c.id
JOIN crm_db.crm_city d ON a.id_city = d.id
LEFT JOIN crm_db.crm_domain e ON a.id_marital_state = e.id
LEFT JOIN crm_db.crm_domain f ON a.id_membership = e.id
LEFT JOIN crm_db.crm_eps g ON a.id_eps = e.id
LEFT JOIN crm_db.crm_occupation h ON a.id_occupation = h.id
WHERE a.id = $P{P_ID_PATIENT}]]>
	</queryString>
