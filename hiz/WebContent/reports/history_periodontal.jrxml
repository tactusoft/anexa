	<parameter name="P_ID_PATIENT" class="java.math.BigDecimal">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT a.id,
    b.start_appointment_date,
	CASE gingivitis
	WHEN '0' THEN 'NO REFERIDO'
	WHEN '1' THEN 'NO'
	WHEN '2' THEN 'SI'
	END gingivitis,
	CASE periodontitis
	WHEN '0' THEN 'NO REFERIDO'
	WHEN '1' THEN 'NO'
	WHEN '2' THEN 'SI'
	END periodontitis,
	CASE mobility
	WHEN '0' THEN 'NO REFERIDO'
	WHEN '1' THEN 'NO'
	WHEN '2' THEN 'SI'
	END mobility,
	CASE bag_teeth
	WHEN '0' THEN 'NO REFERIDO'
	WHEN '1' THEN 'NO'
	WHEN '2' THEN 'SI'
	END bag_teeth,
	CASE gingival_recession
	WHEN '0' THEN 'NO REFERIDO'
	WHEN '1' THEN 'NO'
	WHEN '2' THEN 'SI'
	END gingival_recession,
	CASE plaque_index
	WHEN '0' THEN 'NO REFERIDO'
	WHEN '1' THEN 'NO'
	WHEN '2' THEN 'SI'
	END plaque_index,
    IFNULL(a.obs,'NO REFERIDO') obs
FROM crm_db.crm_odontology_periodontal a JOIN crm_db.crm_appointment b on a.id_appointment = b.id
WHERE b.id_patient = $P{P_ID_PATIENT}
ORDER by b.start_appointment_date DESC]]>
	</queryString>
